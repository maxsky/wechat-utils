{
    "name": "maxsky/wechat-utils",
    "description": "A simply PHP Composer lib For WeChat OfficialAccount/MiniProgram/App/WeCom",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Sky",
            "email": "max940213@live.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "ext-curl": "*",
        "ext-libxml": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "MaxSky\\WeChat\\": "src/"
        },
        "files": [
            "src/Constants/API_WECHAT.php",
            "src/Constants/ERROR_CODE.php",
            "src/Constants/WECHAT_SCENE.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
